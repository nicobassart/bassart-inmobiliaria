        <property name="apellidoDueno" column="apellidoDueno"/>
        <property name="mesanio" column="mesanio"/>
        <property name="valor" column="valor"/>
        <property name="calle" column="calle"/>
        <property name="calleNro" column="calleNro"/>
        <property name="callePiso" column="callePiso"/>
        <property name="calleDpto" column="calleDpto"/>
        <property name="idinmueble" column="idinmueble"/>
        <property name="idalquiler" column="idalquiler"/>
        <property name="porcentajeDueno" column="porcentajeDueno"/>
        <property name="idPersonaDueno" column="idPersonaDueno"/>
        <property name="fecha" column="fecha"/>
        <property name="fechaInicioContrato" column="fechaInicioContrato"/>
        <property name="fechaFinContrato" column="fechaFinContrato"/>
    </class>
</hibernate-mapping>