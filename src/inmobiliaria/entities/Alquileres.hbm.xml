<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="inmobiliaria.entities.Alquileres" table="alquileres">
        <id name="idAlquiler" column="idalquiler">
                <generator class="increment"/>
        </id>
        <property name="idPersonaDueno" column="idPersonaDueno"/>
        <property name="idPersonaInquilino" column="idPersonaInquilino"/>
        <property name="idPersonaGarante" column="idPersonaGarante"/>
        <property name="idInmueble" column="idInmueble"/>
        <property name="fechaInicio" column="fechaInicio"/>
        <property name="fechaFin" column="fechaFin"/>
        <property name="importeDeposito" column="importeDeposito"/>
        <property name="porcentajeDueno" column="porcentajeDueno"/>
        <property name="estado" column="estado"/>
        
        <property name="abl" column="abl"/>
        <property name="gas" column="gas"/>
        <property name="internet" column="internet"/>
        <property name="telefono" column="telefono"/>
        <property name="agua" column="agua"/>
        <property name="cable" column="cable"/>
        
    </class>
</hibernate-mapping>